{
  "$schema": "https://opencode.ai/config.json",
  "username": "faizntfd",
  "autoupdate": true,
  "snapshot": false,
  "plugin": [
    "opencode-gemini-auth@latest",
    "@nick-vi/opencode-type-inject@1.3.1",
    "@franlol/opencode-md-table-formatter@0.0.3"
  ],
  "instructions": [
    "./AGENTS.md"
  ],
  "tui": {
    "scroll_speed": 3,
    "scroll_acceleration": {
      "enabled": true
    },
    "diff_style": "auto"
  },
  "tools": {
    "bash": true,
    "read": true,
    "write": true,
    "edit": true,
    "glob": true,
    "grep": true,
    "list": true,
    "lsp": true,
    "patch": true,
    "skill": true,
    "webfetch": true,
    "task": true,
    "todowrite": true,
    "todoread": true,
    "question": true
  },
  "permission": {
    "bash": "allow",
    "edit": "allow",
    "write": "allow",
    "read": "allow",
    "grep": "allow",
    "glob": "allow",
    "list": "allow",
    "lsp": "allow",
    "patch": "allow",
    "skill": "allow",
    "todowrite": "allow",
    "todoread": "allow",
    "webfetch": "deny",
    "question": "allow"
  },
  "mcp": {
    "zai-mcp-server": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@z_ai/mcp-server"
      ],
      "enabled": false,
      "timeout": 60000,
      "environment": {
        "Z_AI_API_KEY": "XXX",
        "Z_AI_MODE": "ZAI"
      }
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "XXX"
      },
      "enabled": true,
      "timeout": 60000
    },
    "next-devtools": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "next-devtools-mcp@latest"
      ],
      "enabled": true,
      "timeout": 60000
    },
    "Ref": {
      "type": "remote",
      "url": "https://api.ref.tools/mcp?apiKey=XXX",
      "enabled": false,
      "timeout": 60000
    },
    "exa": {
      "type": "remote",
      "url": "https://mcp.exa.ai/mcp?tools=web_search_exa,get_code_context_exa,crawling_exa,company_research_exa,linkedin_search_exa,deep_researcher_start,deep_researcher_check",
      "enabled": false,
      "timeout": 60000,
      "headers": {
        "EXA_API_KEY": "{env:EXA_API_KEY}"
      }
    },
    "supabase": {
      "type": "remote",
      "url": "https://mcp.supabase.com/mcp?project_ref=zxyequaeestijzerrcrx&features=docs%2Caccount%2Cdatabase%2Cdebugging%2Cdevelopment%2Cfunctions%2Cbranching%2Cstorage",
      "enabled": false,
      "timeout": 60000,
      "headers": {
        "Authorization": "Bearer XXX"
      }
    },
    "chrome-devtools": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "enabled": false,
      "timeout": 60000
    },
    "firecrawl-mcp": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "firecrawl-mcp"
      ],
      "environment": {
        "FIRECRAWL_API_KEY": "xxx"
      },
      "enabled": true,
      "timeout": 60000
    },
    "morph-mcp": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@morphllm/morphmcp"
      ],
      "environment": {
        "MORPH_API_KEY": "xxx",
        "ENABLED_TOOLS": "edit_file,warpgrep_codebase_search"
      },
      "enabled": true,
      "timeout": 60000
    }
  },
